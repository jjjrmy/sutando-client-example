  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "knex-cloudflare-d1": "github:jjjrmy/knex-cloudflare-d1#master",
    "nuxt": "^4.1.2",
    "sutando": "^1.7.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "nitro-cloudflare-dev": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "wrangler": "^4.37.0"
  }

{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:worker": "npx nuxi build --preset=cloudflare_module",
    "build:pages": "npx nuxi build --preset=cloudflare_pages",
    "build:ios": "NUXT_SSR=false NUXT_MOBILE=true nuxt generate --devtools --mode static",
    "deploy:worker": "npx wrangler --cwd .output deploy",
    "deploy:pages": "npx wrangler pages deploy dist",
    "secrets:worker": "npx wrangler secret bulk ./secrets.json",
    "secrets:pages": "npx wrangler pages secret bulk ./secrets.json",
    "dev": "nuxt dev",
    "dev:https": "sudo NUXT_SSL=true nuxt dev --host",
    "dev:ios": "bash ./scripts/ios.sh",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "sutando": "sutando",
    "postinstall": "nuxt prepare",
    "seed": "tsx db/seeders/user_posts_seeder.ts"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.3.14",
    "@capacitor-community/apple-sign-in": "^7.0.1",
    "@capacitor-community/stripe": "^7.2.1",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@faker-js/faker": "^8.4.1",
    "@tailwindcss/vite": "^4.1.13",
    "better-auth": "^1.3.11",
    "defu": "^6.1.4",
    "knex-cloudflare-d1": "^0.2.1",
    "nuxt": "^4.1.2",
    "nuxt-security": "^2.4.0",
    "stripe": "^18.5.0",
    "sutando": "^1.7.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-safe-area": "^1.1.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "nitro-cloudflare-dev": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "wrangler": "^4.37.0"
  }
}
